{
  "name": "@browsery/util",
  "version": "0.2.0",
  "description": "Browser compatible NodeJS util module",
  "author": "Panates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/browsery.git",
    "directory": "packages/util"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../build/util"
  },
  "type": "module",
  "module": "./esm/util.mjs",
  "main": "./cjs/util.cjs",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "require": "./cjs/util.cjs",
      "default": "./esm/util.mjs"
    }
  },
  "dependencies": {
    "process": "^0.11.10"
  },
  "devDependencies": {
    "util": "^0.12.5"
  },
  "sideEffects": false,
  "files": [
    "typings/",
    "cjs/",
    "esm/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "util",
    "browser",
    "browsery",
    "browserify"
  ]
}