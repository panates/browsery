{
  "name": "@browsery/stream",
  "version": "0.5.0",
  "description": "Browser compatible NodeJS stream module",
  "author": "Panates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/browsery.git",
    "directory": "packages/stream"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../build/stream"
  },
  "type": "module",
  "module": "./esm/stream.mjs",
  "main": "./cjs/stream.cjs",
  "types": "./typings/index.d.ts",
  "typings": "./typings/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "require": "./cjs/stream.cjs",
      "default": "./esm/stream.mjs"
    }
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "safe-buffer": "^5.2.1",
    "string_decoder": "^1.3.0"
  },
  "devDependencies": {
    "@types/readable-stream": "^2.3.15",
    "readable-stream": "^4.3.0"
  },
  "sideEffects": false,
  "files": [
    "typings/",
    "cjs/",
    "esm/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "stream",
    "browser",
    "browsery",
    "browserify"
  ]
}