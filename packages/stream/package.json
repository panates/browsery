{
  "name": "@browsery/stream",
  "version": "4.7.0-r1",
  "description": "Browser compatible NodeJS stream module",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../build/stream",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "safe-buffer": "^5.2.1",
    "string_decoder": "^1.3.0"
  },
  "devDependencies": {
    "@types/readable-stream": "^4.0.21",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup-extras/plugin-clean": "^1.3.9",
    "ansi-colors": "^4.1.3",
    "readable-stream": "^4.7.0",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-filesize": "^10.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./esm/stream.mjs"
      },
      "require": {
        "types": "./types/index.d.cts",
        "default": "./cjs/stream.cjs"
      },
      "default": "./esm/stream.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/stream.cjs",
  "module": "./esm/stream.mjs",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panates/browsery.git",
    "directory": "packages/stream"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "stream",
    "browser",
    "browsery",
    "browserify"
  ],
  "publishConfig": {
    "access": "public"
  }
}
