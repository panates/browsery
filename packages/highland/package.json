{
  "name": "@browsery/highland",
  "version": "2.13.6-r2",
  "description": "Browser compatible highland module",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../build/highland",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn"
  },
  "dependencies": {
    "@browsery/stream": "^4.3.1-r1",
    "@browsery/util": "^0.12.6-r1",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "string_decoder": "^1.3.0",
    "util-deprecate": "^1.0.2"
  },
  "devDependencies": {
    "@types/highland": "^2.13.0",
    "highland": "^2.13.5"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.cjs",
      "import": "./esm/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/browsery.git",
    "directory": "packages/highland"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "highland",
    "browser",
    "browsery",
    "browserify"
  ]
}
