{
  "name": "@browsery/highland",
  "version": "2.13.6-r4",
  "description": "Browser compatible highland module",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../build/highland",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn"
  },
  "dependencies": {
    "@browsery/stream": "^4.3.1-r3",
    "@browsery/util": "^0.12.6-r2",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "string_decoder": "^1.3.0",
    "util-deprecate": "^1.0.2"
  },
  "devDependencies": {
    "@types/highland": "^2.13.0",
    "@rollup-extras/plugin-clean": "^1.3.9",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-strip": "^3.0.4",
    "ansi-colors": "^4.1.3",
    "highland": "^2.13.5",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-filesize": "^10.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./types/index.d.cts",
        "default": "./cjs/index.cjs"
      },
      "default": "./esm/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/browsery.git",
    "directory": "packages/highland"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "highland",
    "browser",
    "browsery",
    "browserify"
  ]
}
