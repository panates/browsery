{
  "name": "@browsery/jsondiffpatch",
  "version": "0.6.0-r2",
  "description": "Browser compatible jsondiffpatch",
  "author": "Panates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/browsery.git",
    "directory": "packages/jsondiffpatchs"
  },
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../build/jsondiffpatchs",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.cjs",
      "import": "./esm/index.mjs",
      "types": "./types/index.d.ts"
    },
    "./with-text-diffs": {
      "require": "./cjs/with-text-diffs.js",
      "import": "./esm/with-text-diffs.mjs",
      "types": "./types/with-text-diffs.d.ts"
    },
    "./formatters/*": {
      "require": "./cjs/formatters/*.cjs",
      "import": "./esm/formatters/*.mjs",
      "types": "./types/formatters/*.d.ts"
    }
  },
  "dependencies": {
    "jsondiffpatch": "^0.6.0"
  },
  "sideEffects": false,
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "jsondiffpatch",
    "jsonpatch",
    "json",
    "diff",
    "patch"
  ]
}
