{
  "name": "@browsery/antlr4",
  "version": "4.13.2",
  "description": "Browser compatible antlr4 module",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "node build.mjs",
    "clean": "rimraf ../../build/antlr4",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn"
  },
  "devDependencies": {
    "antlr4": "^4.13.2"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/antlr4.node.mjs",
        "require": "./dist/antlr4.node.cjs",
        "types": "./src/antlr4/index.d.cts",
        "default": "./dist/antlr4.node.mjs"
      },
      "browser": {
        "import": "./dist/antlr4.web.mjs",
        "require": "./dist/antlr4.web.cjs",
        "types": "./src/antlr4/index.d.cts",
        "default": "./dist/antlr4.web.mjs"
      },
      "require": "./dist/antlr4.node.cjs"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/browsery.git",
    "directory": "packages/antlr4"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "antlr4",
    "browser",
    "browsery",
    "browserify"
  ]
}
