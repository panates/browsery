{
  "name": "@browsery/antlr4",
  "version": "4.13.3-r2",
  "description": "Browser compatible antlr4 module",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "node build.mjs",
    "clean": "rimraf ../../build/antlr4",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn"
  },
  "devDependencies": {
    "antlr4": "^4.13.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./esm/antlr4.node.mjs"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./cjs/antlr4.node.cjs"
      },
      "node": {
        "types": "./types/index.d.ts",
        "import": "./esm/antlr4.node.mjs",
        "require": "./cjs/antlr4.node.cjs",
        "default": "./esm/antlr4.node.mjs"
      },
      "browser": {
        "types": "./types/index.d.ts",
        "import": "./esm/antlr4.web.mjs",
        "require": "./cjs/antlr4.web.cjs",
        "default": "./esm/antlr4.web.mjs"
      },
      "default": "./esm/antlr4.node.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/antlr4.web.cjs",
  "module": "./esm/antlr4.web.mjs",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/browsery.git",
    "directory": "packages/antlr4"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "antlr4",
    "browser",
    "browsery",
    "browserify"
  ]
}
